<?xml version='1.0' encoding='UTF-8'?>
<pyvcp>
	<vbox>
		<relief>RIDGE</relief>
		<bd>6</bd>
		<hbox>
			<label>
				<text>"Target RMP: "</text>
				<font>("Helvetica",18)</font>
			</label>
			<hbox>
				<relief>RIDGE</relief>
				<bd>4</bd>
				<number>
					<halpin>"spindle-rpm"</halpin>
					<format>"05d"</format>
					<font>("Arial",20)</font>
				</number>
			</hbox>
		</hbox>
		<label>
			<text>"Spindle Speed:"</text>
			<font>("Helvetica",18)</font>
		</label>
		<hbox>
			<bar>
				<halpin>"spindle-speed"</halpin>
				<max_>24000</max_>
			</bar>
			<led>
				<halpin>"spindle-at-speed-led"</halpin>
				<size>30</size>
				<on_color>"green"</on_color>
				<off_color>"red"</off_color>
			</led>
		</hbox>
		<vbox>
			<label>
				<text>"Spindle Power"</text>
			</label>
			<bar>
				<halpin>"spindle-power"</halpin>
				<max_>2200</max_>
			</bar>
			<label>
				<text>"Spindle Voltage"</text>
			</label>
			<bar>
				<halpin>"spindle-voltage"</halpin>
				<max_>220</max_>
			</bar>
			<label>
				<text>"Spindle Current"</text>
			</label>
			<bar>
				<halpin>"spindle-current"</halpin>
				<max_>20</max_>
			</bar>
			<label>
				<text>"DC Boltage"</text>
			</label>
			<bar>
				<halpin>"vfd-dc-bus"</halpin>
				<max_>320</max_>
			</bar>
			<label>
				<text>"Temperature"</text>
			</label>
			<bar>
				<halpin>"vfd-temperature"</halpin>
				<max_>100</max_>
			</bar>
		</vbox>
		<vbox>
			<label>
				<text>"Spindle torque"</text>
			</label>
			<bar>
				<halpin>"spindle-torque"</halpin>
				<format>"0.2f"</format>
				<max_>1.5</max_>
			</bar>
			<checkbutton>
				<halpin>"laser-chkbtn"</halpin>
				<text>"laser"</text>
			</checkbutton>
		</vbox>
		<hbox>
			<vbox>
				<hbox>
					<led>
						<halpin>"s0"</halpin>
					</led>
					<label>
						<text>"powering off"</text>
					</label>
				</hbox>
				<hbox>
					<led>
						<halpin>"s1"</halpin>
					</led>
					<label>
						<text>"stopping"</text>
					</label>
				</hbox>
				<hbox>
					<led>
						<halpin>"s2"</halpin>
					</led>
					<label>
						<text>"running"</text>
					</label>
				</hbox>
				<hbox>
					<led>
						<halpin>"s3"</halpin>
					</led>
					<label>
						<text>"startf"</text>
					</label>
				</hbox>
				<hbox>
					<led>
						<halpin>"s4"</halpin>
					</led>
					<label>
						<text>"self tune"</text>
					</label>
				</hbox>
				<hbox>
					<led>
						<halpin>"s5"</halpin>
					</led>
					<label>
						<text>"operating"</text>
					</label>
				</hbox>
				<hbox>
					<led>
						<halpin>"s6"</halpin>
					</led>
					<label>
						<text>"ready"</text>
					</label>
				</hbox>
				<hbox>
					<led>
						<halpin>"s10"</halpin>
					</led>
					<label>
						<text>"fault"</text>
					</label>
				</hbox>
				<hbox>
					<led>
						<halpin>"s11"</halpin>
					</led>
					<label>
						<text>"alarm"</text>
					</label>
				</hbox>
				<hbox>
					<led>
						<halpin>"s12"</halpin>
					</led>
					<label>
						<text>"STO state"</text>
					</label>
				</hbox>
			</vbox>
			<vbox>
				<hbox>
					<led>
						<halpin>"f1"</halpin>
					</led>
					<label>
						<text>"abnormal"</text>
					</label>
				</hbox>
				<hbox>
					<led>
						<halpin>"f4"</halpin>
					</led>
					<label>
						<text>"GND fault"</text>
					</label>
				</hbox>
				<hbox>
					<led>
						<halpin>"f5"</halpin>
					</led>
					<label>
						<text>"GND short"</text>
					</label>
				</hbox>
				<hbox>
					<led>
						<halpin>"f6"</halpin>
					</led>
					<label>
						<text>"over curr"</text>
					</label>
				</hbox>
				<hbox>
					<led>
						<halpin>"f7"</halpin>
					</led>
					<label>
						<text>"out overV"</text>
					</label>
				</hbox>
				<hbox>
					<led>
						<halpin>"f8"</halpin>
					</led>
					<label>
						<text>"DC overV"</text>
					</label>
				</hbox>
				<hbox>
					<led>
						<halpin>"f9"</halpin>
					</led>
					<label>
						<text>"DC underV"</text>
					</label>
				</hbox>
				<hbox>
					<led>
						<halpin>"f10"</halpin>
					</led>
					<label>
						<text>"VFD overH"</text>
					</label>
				</hbox>
				<hbox>
					<led>
						<halpin>"f13"</halpin>
					</led>
					<label>
						<text>"bridge overh"</text>
					</label>
				</hbox>
				<hbox>
					<led>
						<halpin>"f14"</halpin>
					</led>
					<label>
						<text>"U phase miss"</text>
					</label>
				</hbox>
			</vbox>
		</hbox>
		<hbox>
			<button>
				<font>("Helvetica",16)</font>
				<halpin>"fault-reset"</halpin>
				<text>"Fault reset"</text>
			</button>
			<u32>
				<halpin>"comm-fault-last"</halpin>
				<format>"02d"</format>
				<font>("Arial",20)</font>
			</u32>
			<led>
				<halpin>"comm-fault"</halpin>
				<size>30</size>
				<on_color>"red"</on_color>
				<off_color>"green"</off_color>
			</led>
		</hbox>
	</vbox>
</pyvcp>
